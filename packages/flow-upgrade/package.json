{
  "name": "flow-upgrade",
  "version": "1.1.1",
  "description": "A utility for upgrading your codebase to the latest version of Flow.",
  "engines": {
    "node": ">=6"
  },
  "files": [
    "dist"
  ],
  "bin": "./dist/index.js",
  "scripts": {
    "build": "rm -rf dist && babel src --out-dir dist --ignore \"src/**/__tests__/*\"",
    "test": "jest --watch",
    "flow": "flow",
    "prepublish": "npm run build",
    "postpublish": "node ./scripts/postpublish.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/facebook/flow.git"
  },
  "bugs": {
    "url": "https://github.com/facebook/flow/issues"
  },
  "homepage": "https://github.com/facebook/flow/tree/master/packages/flow-upgrade#readme",
  "jest": {
    "rootDir": "src",
    "testMatch": [
      "**/__tests__/*-test.js"
    ]
  },
  "dependencies": {
    "chalk": "^2.4.2",
    "graceful-fs": "^4.1.11",
    "jscodeshift": "https://github.com/jbrown215/jscodeshift.git",
    "ora": "^1.4.0",
    "prompt-confirm": "^1.2.0",
    "semver": "^5.3.0",
    "yargs": "^8.0.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.7.7",
    "@babel/core": "^7.7.7",
    "@babel/plugin-proposal-class-properties": "^7.7.4",
    "@babel/preset-env": "^7.8.3",
    "@babel/preset-flow": "^7.8.3",
    "flow-bin": "0.118.0",
    "jest": "^25.1.0"
  },
  "resolutions": {
    "lodash": ">=4.17.14",
    "recast": "https://github.com/moroine/recast.git#6714f115bdc27463f416e3d2062e6e2f28e67266"
  }
}
