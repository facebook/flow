function Clock(props): any {
  return (
    <div><h1>Hello, world!</h1><h2>It is {props.date.toLocaleTimeString()}.</h2></div>
  );
}

function tick(): void {
  const element = document.getElementById("root");
  if (element) {
    ReactDOM.render(<Clock date={new Date()} />, element);
  }
}

setInterval(tick, 1000);

