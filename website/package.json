    "babel-loader": "^8.2.2",
    "css-loader": "^6.2.0",
    "cssnano": "^5.0.7",
    "expose-loader": "^3.0.0",
    "file-loader": "^6.2.0",
    "mini-css-extract-plugin": "^2.1.0",
